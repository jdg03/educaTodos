<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="./css/stylesolicitudes.css">
    <link rel="stylesheet" href="./css/styleheader.css">
    <title>Solicitudes</title>
</head>
<body class="fondo">
   
    <!-- sección menu -->
    <header class="header">
        <div class="logo">
            <img src="../img/EDUCATODOS.png" alt="">
        </div>
        <nav>
            <ul class="nav-links">
                <li><a href="/bienvenidoAdmi">Inicio</a></li>
                <li><a href="#">Matricula</a></li>
                <li><a href="/expedientesEstudiantiles">/</a></li>
                <li><a href="#">Traslado</a></li>
                <li><a href="#">Instituciones</a></li>
                <li><a href="#">Solicitudes</a></li>
            </ul>
        </nav>

        <span>
            <a class="btn"><button><%= user.nombre%>  <%=user.apellido%></button></a>
        </span>
      
        <button class="navbar_toogle-btn">
            <img src="./img/puntos.ico" alt="">
        </button>
        <div class="navbar_mobile-menu">
            <ul class="nav-list-mobile">
                <li class="nav_link-mobile"><a href="bienvenidoAdmi">Inicio</a></li>
                <li class="nav_link-mobile"><a href="#">Matricula</a></li>
                <li class="nav_link-mobile"><a href="/expedientesEstudiantiles">Expediente Estudiantil</a></li>
                <li class="nav_link-mobile"><a href="#">Traslado</a></li>
                <li class="nav_link-mobile"><a href="#">Instituciones</a></li>
                <li class="nav_link-mobile"><a href="#">Solicitudes</a></li>
            </ul>
        </div>
        </nav>
    </header>

    <!-- sección solicitudes -->
    <div class="bienvenida">
        <h2>Solicitudes</h2>
        <div class="atras">
            <button type="submit">Anterior</button>
        </div>
        <div class="cerrar">
            <a href="/logout">Cerrar Sesión</a>
        </div>
    </div>
    <hr>

    <!-- sección tipos solicitudes -->
    <section id="servicios" class="servicios">
        <div class="fila">
            <div class="col">
                <div class="card">
                    <div class="icono">
                        <img src="./img/expediente.ico" class="img">
                    </div>
                    <h3>Expedientes estudiantiles</h3>
                    <button class="button" name="expediente" type="submit">Ver más</button>
                </div>
            </div>
            <div class="col">
                <div class="card">
                    <div>
                        <img src="img/traslado.ico" class="img">
                    </div>
                    <h3>Traslados</h3>
                    <button class="button" name="traslado" type="submit">Ver más</button>
                </div>
            </div>
            <div class="col">
                <div class="card">
                    <div>
                        <img src="img/solicitudes.ico" class="img">
                    </div>
                    <h3>Más Solicitudes</h3>
                    <button class="button" name="profesores" type="submit">Ver más</button>
                </div>
            </div>

    </section>

    <!-- sección footer -->
    <footer>
        <div class="footerBottom">
            <p>EDUCATODOS &copy;2024 Diseñado por <span class="designer">Grupo 5</span></p>
        </div>
    </footer>

    <script src="./js/main.js"></script>  
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.min.js"></script>
</body>
</html>